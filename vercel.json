{
    "builds": [
        {
            "src": "./build/app.js",
            "use": "@vercel/node"
        },
        {
            "src": "./views/out",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "./build/app.js"
        }
    ]
}