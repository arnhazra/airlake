{
    "builds": [
        {
            "src": "api/handler.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "api/**"
                ]
            }
        },
        {
            "src": "./client/out",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/handler.js"
        }
    ]
}