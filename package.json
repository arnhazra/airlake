    "@openzeppelin/contracts": "^4.8.1",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.3",
    "googleapis": "^111.0.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.9.1",
    "nodemailer": "^6.9.1",
    "nodemon": "^2.0.20",
    "otp-without-db": "^1.0.4",
    "superagent": "^8.0.9"
  }
}