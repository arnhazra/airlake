{
  "name": "project-lenstack",
  "version": "1.3",
  "description": "Project Lenstack",
  "main": "app.js",
  "scripts": {
    "initialize": "npm install && npm install --prefix views",
    "start": "node app",
    "api": "nodemon app",
    "views": "npm start --prefix views",
    "client": "npm run dev --prefix client",
    "dev": "concurrently \"npm run api\" \"npm run client\"",
    "deploy": "npm run build --prefix views && git fetch -a && git checkout master && git add . && git commit -m autodeploy && git push origin master"
  },
  "keywords": [],
  "author": "Arnab Hazra",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.3",
    "fuse.js": "^6.6.2",
    "googleapis": "^111.0.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.1",
    "nodemailer": "^6.9.1",
    "otp-generator": "^4.0.1",
    "otp-without-db": "^1.0.4",
    "redis": "^4.6.5",
    "superagent": "^8.0.9"
  },
  "devDependencies": {
    "@openzeppelin/contracts": "^4.8.1",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20"
  }
}